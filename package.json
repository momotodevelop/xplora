{
  "name": "xplora",
  "version": "2.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:xplora": "node dist/xplora/server/server.mjs",
    "prerender": "ng run xplora:prerender",
    "lint": "ng lint",
    "extract-i18n": "ng extract-i18n",
    "serve:dynamic": "ng serve --configuration development --host 0.0.0.0 --disable-host-check",
    "build:dynamic": "ng build --configuration production",
    "build:prod-ssr": "ng build --configuration production",
    "build:functions": "npm install --force --prefix functions && npm run build --prefix functions",
    "copy:server-bundle": "rm -rf functions/lib/ssr-bundle && mkdir -p functions/lib/ssr-bundle && cp -R dist/xplora/server/* functions/lib/ssr-bundle/",
    "deploy:firebase": "npm run build:prod-ssr && npm run build:functions && npm run copy:server-bundle && firebase deploy --only hosting,functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.2.8",
    "@angular/cdk": "19.2.11",
    "@angular/common": "~19.2.8",
    "@angular/compiler": "~19.2.8",
    "@angular/core": "~19.2.8",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "~19.2.8",
    "@angular/google-maps": "^19.2.11",
    "@angular/material": "19.2.11",
    "@angular/platform-browser": "~19.2.8",
    "@angular/platform-browser-dynamic": "~19.2.8",
    "@angular/platform-server": "~19.2.8",
    "@angular/router": "~19.2.8",
    "@angular/service-worker": "~19.2.8",
    "@angular/ssr": "^19.2.9",
    "@fingerprintjs/fingerprintjs-pro-angular": "^2.0.0",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@mercadopago/sdk-js": "^0.0.3",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@popperjs/core": "^2.11.8",
    "@sanity/client": "^6.19.1",
    "@sanity/types": "^3.45.0",
    "@sweetalert2/ngx-sweetalert2": "^13.0.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/humanize-duration": "^3.27.4",
    "@types/uuid": "^10.0.0",
    "angular-cc-library": "^3.4.0",
    "angular-feather": "^6.5.1",
    "bootstrap": "^5.3.2",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "firebase": "^11.4.0",
    "humanize-duration": "^3.31.0",
    "iv-viewer": "^2.2.1",
    "lodash": "^4.17.21",
    "lottie-web": "^5.12.2",
    "moment": "^2.30.1",
    "ngx-countdown": "^19.0.0",
    "ngx-export-as": "^1.19.0",
    "ngx-extended-pdf-viewer": "^22.0.0",
    "ngx-lottie": "^13.0.0",
    "ngx-print": "^3.0.0",
    "ngx-skeleton-loader": "^9.0.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.16.1",
    "swiper": "^11.2.8",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.15.0"
